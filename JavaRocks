public class JavaRocks {
      
       /*  This program prints my initials (DJE) in big letters,
           where each letter is nine lines tall.
       */
    
       public static void main(String[] args) {
          System.out.println();
          System.out.println("MM   MM Y    Y       J      A       V      V      A ");
          System.out.println("M MMM M  Y  Y        J     A A       V    V      A A ");
          System.out.println("M  M  M   YY      J  J    AAAAA       V  V      AAAAA ");
          System.out.println("M     M   YY       JJ    A     A       VV      A     A ");
          System.out.println("");
          System.out.println("");
          System.out.println("");
          System.out.println("");
          System.out.println("U     U  SSSSS  IIIIII  N       N   GGGG ");
	  System.out.println("U     U  S        II    NN      N   G ");
	  System.out.println("U     U   SS      II    N N     N   G GGG ");
	  System.out.println(" U   U       S    II    N  NN   N   G   G ");
	  System.out.println("  UUU     SSSS    II    N    NNNN   GGGGG ");
          System.out.println("");
          System.out.println("");
          System.out.println("");
          System.out.println("");
          System.out.println("TTTTTTT  EEEEE RRRRRR  M      M ");
	  System.out.println("   T     E     R    R  MM    MM ");
	  System.out.println("   T     EEEE  R   R   M M  M M ");
	  System.out.println("   T     E     RRRRR   M  MM  M ");
	  System.out.println("   T     EEEEE R    R  M  MM  M ");
       } 

	/* I used the terminal in a mac book to compile this program and the quote thing was the biggest issue */ 

    };